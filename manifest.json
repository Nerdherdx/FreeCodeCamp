{
  "manifest_version": 2,
  "name": "Hide hint",
  "version": "0.1",
  "icons": {
    "128": "question.png"
  },
  "browser_action":{
    "default_icon":"question.png",
  "default_popup":"hide.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["style.css"],
      "js": ["jquery-2.1.4.min.js", "js/content.js"]
    }
  ]
}

